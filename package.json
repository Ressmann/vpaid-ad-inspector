{
  "name": "vpaid-ad-inspector",
  "version": "1.0.0",
  "description": "Simple VPAID ad to inspect",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src",
    "browserify": "browserify src/index.js -o dist/vpaid-ad-inspector.js",
    "uglify": "uglifyjs dist/vpaid-ad-inspector.js -o dist/vpaid-ad-inspector.min.js -c -m --screw-ie8",
    "build": "npm run lint && npm run browserify && npm run uglify",
    "watchify": "watchify src/index.js -o dist/vpaid-ad-inspector.js",
    "test": "mocha ."
  },
  "dependencies": {
    "vpaid-ad": "^1.0.7"
  },
  "devDependencies": {
    "babel": "^6.1.18",
    "babel-core": "^6.2.1",
    "babel-eslint": "^4.1.6",
    "babel-preset-es2015": "^6.1.18",
    "babelify": "^7.2.0",
    "brfs": "^1.4.1",
    "browserify": "^13.0.0",
    "eslint": "^1.10.1",
    "eslint-config-standard": "^5.0.0",
    "eslint-plugin-standard": "^1.3.1",
    "gulp": "^3.9.0",
    "mocha": "^2.3.4",
    "uglify-js": "^2.6.1"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ],
      "brfs"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kahwee/vpaid-ad-inspector.git"
  },
  "author": "KahWee Teng <t@kw.sg>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kahwee/vpaid-ad-inspector/issues"
  },
  "homepage": "https://github.com/kahwee/vpaid-ad-inspector#readme",
  "keywords": [
    "vpaid",
    "ad",
    "adtech",
    "vast",
    "debugging",
    "advertisement",
    "unit"
  ]
}
